{
  "name": "shopify-node-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prod": "npm run clean && npm run build && cross-env NODE_ENV=production npm run go",
    "dev": "NODE_ENV=development npm run go",
    "go": "node ./bin/www",
    "start": "npm run clean && npm run build && NODE_ENV=production npm run go",
    "build": "NODE_ENV=production webpack --mode production --config ./config/webpack.config.js --progress --profile --colors",
    "clean": "rm -rf assets && mkdir assets",
    "pretty": "prettier --single-quote --trailing-comma es5 --write {client,bin,config,server}{/*,/**/*}.js",
    "precommit": "npm run pretty",
    "heroku-prebuild": "npm install --production=false",
    "postinstall": "NODE_ENV=production webpack --mode production --config ./config/webpack.config.js"
  },
  "engines": {
    "node": ">= 8.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "@shopify/polaris": "^3.1.1",
    "autoprefixer": "^7.1.1",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browserslist": "^4.3.4",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "connect-redis": "^3.4.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.4",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "ejs": "~2.5.6",
    "eslint-config-react-app": "^3.0.5",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "global": "^4.3.2",
    "isomorphic-fetch": "^2.2.1",
    "knex": "^0.13.0",
    "moment": "^2.22.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.17.1",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.5.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-moment": "^0.7.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^2.1.1",
    "redis": "^2.8.0",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "shopify-api-node": "^2.11.0",
    "shopify-express-rdj": "^1.0.12-alpha.8",
    "style-loader": "^0.18.2",
    "url": "^0.11.0",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.18.0",
    "yarn": "^1.12.3"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-plugin-prettier": "^2.1.2"
  }
}
